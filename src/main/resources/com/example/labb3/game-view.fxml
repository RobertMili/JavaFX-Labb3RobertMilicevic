<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.canvas.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane prefHeight="500.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.labb3.GameViewController">
   <top>
      <HBox alignment="CENTER_LEFT" prefHeight="48.0" prefWidth="500.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="cirkelButton" onAction="#onCirkelClicked" text="Cirkel" />
            <Button fx:id="rectangleButton" onAction="#onRectangleClicked" text="Rectangle" />
            <ColorPicker fx:id="colorPicked" />
            <Button mnemonicParsing="false" text="Button" />
            <Button mnemonicParsing="false" text="Button" />
         </children></HBox>
   </top>
   <left>
      <Canvas fx:id="canvas" height="448.0" onMouseClicked="#canvasClicked" width="494.0" BorderPane.alignment="CENTER" />
   </left>



</BorderPane>
