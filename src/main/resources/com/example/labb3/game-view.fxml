<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.canvas.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="700.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.labb3.GameViewController">
   <HBox alignment="CENTER_LEFT" prefHeight="48.0" prefWidth="500.0">
      <children>
         <ToggleButton  mnemonicParsing="false" text="Cirkel"        fx:id="cirkelButton"/>
         <ToggleButton mnemonicParsing="false" text="Rectangle"      fx:id="rectangleButton" />
         <ColorPicker fx:id="colorPicked" />
         <Button mnemonicParsing="false" text="Undo" />
         <TextField fx:id="sizeTextField" onAction="#sizeTextField" prefHeight="25.0" prefWidth="76.0" />


      </children>
   </HBox>
   <Pane layoutX="-1.0" layoutY="46.0" prefHeight="633.0" prefWidth="690.0">
      <children>
         <Canvas fx:id="canvas" height="643.0" width="690.0" layoutX="11.0" layoutY="4.0" onMouseClicked="#canvasClicked"  BorderPane.alignment="TOP_LEFT" />
      </children>
   </Pane>
</AnchorPane>
